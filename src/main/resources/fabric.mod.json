{
  "schemaVersion": 1,
  "id": "interdimensional",
  "version": "${version}",

  "name": "Interdimensional",
  "description": "A runtime dimensions mod for fabric",
  "authors": [
    "yitzy299"
  ],
  "contact": {
    "homepage": "https://github.com/QuiltServerTools/Interdimensional",
    "sources": "https://github.com/QuiltServerTools/Interdimensional"
  },
  "license": "LGPL-3.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "net.quiltservertools.interdimensional.Interdimensional::INSTANCE"
      }
    ]
  },
  "mixins": [
    "interdimensional.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.6",
    "fabric": ">=0.43.1",
    "minecraft": "1.18.x",
    "fantasy": ">=0.4.3",
    "fabric-language-kotlin": ">=1.6.0",
    "interdimensional-portals-api": "*"
  },

  "breaks": {
    "cardboard": "*",
    "multiworld": "*"
  }
}
